{
    "builds": [
      {
      "src": "main.py",
      "use": "@vercel/python",
      "config": {
        "pythonVersion": "3.10"
      }
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": { "distDir": "staticfiles" }
    }
  ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
      "src": "/(.*)",
      "dest": "main.py"
    },
    ]
}
